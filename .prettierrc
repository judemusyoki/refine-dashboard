{
  "tabWidth": 2,
  "semi": false,
  "singleQuote": true,
  "trailingComma": "all",
  "printWidth": 80,
  "useTabs": false,
  "endOfLine": "auto",
  "importOrder": [
    "^reflect-metadata",
    "^react(.*)$",
    "^next(.*)$",
    "^@mui/(.*)$",
    "^@/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true
}
